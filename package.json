{
	"name": "smogon-usage-fetch",
	"version": "5.0.4",
	"description": "JavaScript API to fetch smogon usage stats",
	"keywords": [
		"smogon",
		"pokemon",
		"api"
	],
	"homepage": "https://github.com/FelixRilling/smogon-usage-fetch#readme",
	"bugs": {
		"url": "https://github.com/FelixRilling/smogon-usage-fetch/issues"
	},
	"license": "MIT",
	"author": {
		"name": "Felix Rilling",
		"email": "contact@rilling.dev",
		"url": "https://rilling.dev"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FelixRilling/smogon-usage-fetch.git"
	},
	"type": "module",
	"exports": "./dist/main.js",
	"main": "./dist/main.js",
	"scripts": {
		"start": "npm run dist",
		"dist": "npm run lint && npm run clean && npm run build && npm run test && npm run docs",
		"clean": "rm -rf ./dist",
		"build": "tsc --build ./src/tsconfig.json",
		"watch": "tsc --build ./src/tsconfig.json -w",
		"lint": "eslint ./src/ ./__tests__/ --ext .ts",
		"fix": "eslint ./src/ ./__tests__/ --ext .ts --fix",
		"test": "jest",
		"docs": "typedoc",
		"test:manual": "ts-node-esm __tests__/manual/main.ts"
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.12",
		"lightdash": "^14.0.0-4"
	},
	"devDependencies": {
		"@types/jest": "^29.0.3",
		"@typescript-eslint/eslint-plugin": "^5.18.0",
		"@typescript-eslint/parser": "^5.18.0",
		"eslint": "^8.13.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "^29.0.3",
		"prettier": "^2.6.2",
		"ts-jest": "^29.0.1",
		"ts-node": "^10.9.1",
		"typedoc": "^0.23.8",
		"typescript": "~4.9.4"
	},
	"engines": {
		"node": ">=18"
	}
}
