{
    "extends": "eslint:recommended",
    "plugins": [],
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "jasmine": true
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8
    },
    "rules": {
        "no-console": 1,
        "no-else-return": 2,
        "consistent-return": 2,
        "no-extra-parens": 1,
        "block-scoped-var": 1
    }
}
